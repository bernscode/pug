#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/fsevents/1.1.3/node_modules/fsevents/node_modules/sshpk/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/fsevents/1.1.3/node_modules/fsevents/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/fsevents/1.1.3/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/fsevents/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules:/Users/bernscode/projects/web_dev_practice/node_modules:/Users/bernscode/projects/node_modules:/Users/bernscode/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../sshpk/bin/sshpk-sign" "$@"
  ret=$?
else 
  NODE_PATH="/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/fsevents/1.1.3/node_modules/fsevents/node_modules/sshpk/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/fsevents/1.1.3/node_modules/fsevents/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/fsevents/1.1.3/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/fsevents/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules:/Users/bernscode/projects/web_dev_practice/node_modules:/Users/bernscode/projects/node_modules:/Users/bernscode/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../sshpk/bin/sshpk-sign" "$@"
  ret=$?
fi
exit $ret
