#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/sshpk/1.13.1/node_modules/sshpk/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/sshpk/1.13.1/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/sshpk/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules:/Users/bernscode/projects/web_dev_practice/node_modules:/Users/bernscode/projects/node_modules:/Users/bernscode/node_modules:/Users/node_modules:/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/http-signature/1.1.1/node_modules/sshpk/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/http-signature/1.1.1/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/http-signature/node_modules" "$basedir/node"  "$basedir/../../../sshpk/bin/sshpk-verify" "$@"
  ret=$?
else 
  NODE_PATH="/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/sshpk/1.13.1/node_modules/sshpk/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/sshpk/1.13.1/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/sshpk/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules:/Users/bernscode/projects/web_dev_practice/node_modules:/Users/bernscode/projects/node_modules:/Users/bernscode/node_modules:/Users/node_modules:/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/http-signature/1.1.1/node_modules/sshpk/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/http-signature/1.1.1/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/http-signature/node_modules" node  "$basedir/../../../sshpk/bin/sshpk-verify" "$@"
  ret=$?
fi
exit $ret
