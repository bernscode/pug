#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/esprima-fb/15001.1001.0-dev-harmony-fb/node_modules/esprima-fb/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/esprima-fb/15001.1001.0-dev-harmony-fb/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/esprima-fb/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules:/Users/bernscode/projects/web_dev_practice/node_modules:/Users/bernscode/projects/node_modules:/Users/bernscode/node_modules:/Users/node_modules:/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/recast/0.10.33/node_modules/esprima-fb/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/recast/0.10.33/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/recast/node_modules" "$basedir/node"  "$basedir/../../../esprima-fb/bin/esvalidate.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/esprima-fb/15001.1001.0-dev-harmony-fb/node_modules/esprima-fb/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/esprima-fb/15001.1001.0-dev-harmony-fb/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/esprima-fb/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules:/Users/bernscode/projects/web_dev_practice/node_modules:/Users/bernscode/projects/node_modules:/Users/bernscode/node_modules:/Users/node_modules:/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/recast/0.10.33/node_modules/esprima-fb/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/recast/0.10.33/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/recast/node_modules" node  "$basedir/../../../esprima-fb/bin/esvalidate.js" "$@"
  ret=$?
fi
exit $ret
