#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/strip-bom/1.0.0/node_modules/strip-bom/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/strip-bom/1.0.0/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/strip-bom/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules:/Users/bernscode/projects/web_dev_practice/node_modules:/Users/bernscode/projects/node_modules:/Users/bernscode/node_modules:/Users/node_modules:/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/vinyl-fs/0.3.14/node_modules/strip-bom/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/vinyl-fs/0.3.14/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/vinyl-fs/node_modules" "$basedir/node"  "$basedir/../../../strip-bom/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/strip-bom/1.0.0/node_modules/strip-bom/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/strip-bom/1.0.0/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/strip-bom/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules:/Users/bernscode/projects/web_dev_practice/node_modules:/Users/bernscode/projects/node_modules:/Users/bernscode/node_modules:/Users/node_modules:/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/vinyl-fs/0.3.14/node_modules/strip-bom/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/vinyl-fs/0.3.14/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/vinyl-fs/node_modules" node  "$basedir/../../../strip-bom/cli.js" "$@"
  ret=$?
fi
exit $ret
