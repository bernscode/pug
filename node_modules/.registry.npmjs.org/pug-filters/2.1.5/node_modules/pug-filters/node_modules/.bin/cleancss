#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/clean-css/3.4.28/node_modules/clean-css/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/clean-css/3.4.28/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/clean-css/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules:/Users/bernscode/projects/web_dev_practice/node_modules:/Users/bernscode/projects/node_modules:/Users/bernscode/node_modules:/Users/node_modules:/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/pug-filters/2.1.5/node_modules/clean-css/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/pug-filters/2.1.5/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/pug-filters/node_modules" "$basedir/node"  "$basedir/../../../clean-css/bin/cleancss" "$@"
  ret=$?
else 
  NODE_PATH="/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/clean-css/3.4.28/node_modules/clean-css/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/clean-css/3.4.28/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/clean-css/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules:/Users/bernscode/projects/web_dev_practice/node_modules:/Users/bernscode/projects/node_modules:/Users/bernscode/node_modules:/Users/node_modules:/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/pug-filters/2.1.5/node_modules/clean-css/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/pug-filters/2.1.5/node_modules:/Users/bernscode/projects/web_dev_practice/pug/node_modules/.registry.npmjs.org/pug-filters/node_modules" node  "$basedir/../../../clean-css/bin/cleancss" "$@"
  ret=$?
fi
exit $ret
